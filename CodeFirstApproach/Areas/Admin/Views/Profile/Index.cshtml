@model CodeFirstApproach.Identity.ApplicationUser
@{
    ViewBag.Title = "My Profile";
}

<div class="w-70 m-auto">
    <h2 class="txt-center mb-5">My Profile</h2>

    <div class="d-grid grid-1-4 mb-3">
        <h6>Username</h6>
        <p>@Model.UserName</p>
    </div>
    <div class="d-grid grid-1-4 mb-3">
        <h6>Email</h6>
        <p>@Model.Email</p>
    </div>
</div>